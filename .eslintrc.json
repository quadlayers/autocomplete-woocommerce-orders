{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:@wordpress/eslint-plugin/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"react/prop-types": "off",
		"jsdoc/check-tag-names": "off",
		"jsx-a11y/label-has-associated-control": "off",
		"import/no-unresolved": ["error", { "ignore": ["^jquery$"] }]
	},
	"settings": {
		"react": {
			"version": "999.999.999"
		}
	}
}
