{
	"name": "quadlayers/autocomplete-woocommerce-orders",
	"description": "Autocomplete WooCommerce Orders",
	"type": "project",
	"authors": [
		{
			"name": "QuadLayers",
			"email": "noreply@quadlayers.com"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"platform": {
			"php": "7.2.0"
		},
		"allow-plugins": {
			"quadlayers/wp-autoload": true,
			"automattic/jetpack-composer-plugin": true
		}
	},
	"require": {
		"php": ">=7.2.0",
		"automattic/jetpack-composer-plugin": "*",
		"automattic/jetpack-assets": "*",
		"quadlayers/wp-dashboard-widget-news": "*",
		"quadlayers/wp-notice-plugin-required": "*",
		"quadlayers/wp-notice-plugin-promote": "*",
		"quadlayers/wp-plugin-table-links": "*",
		"quadlayers/wp-plugin-suggestions": "*",
		"quadlayers/wp-plugin-install-tab": "*",
		"franmastromarino/wp-plugin-feedback": "dev-php7.2"
	},
	"autoload": {
		"classmap": [
			"lib/"
		]
	},
	"autoload-dev": {
		"exclude-from-classmap": [
			"lib/"
		]
	},
	"require-dev": {
		"quadlayers/wp-autoload": "*"
	},
	"extra": {
		"quadlayers/wp-autoload": {
			"QuadLayers\\ACO": "lib/"
		},
		"wp-plugin-slug": "autocomplete-woocommerce-orders"
	},
	"scripts": {}
}
